{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "static/$1"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      },
      "dest": "static/logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}